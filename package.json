{
  "private": "true",
  "type": "module",
  "name": "ack",
  "version": "0.0.1",
  "description": "Add's Creative Kit, an object oriented abstraction to provide better control over SVGs.",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "spec",
    "doc": "docs"
  },
  "scripts": {
    "coverage": "nyc jasmine-browser-runner runSpecs",
    "test": "jasmine-browser-runner serve",
    "test-cli": "jasmine spec/Image.spec.js",
    "lint": "semistandard --env jasmine",
    "document": "jsdoc -r -d docs/ src",
    "format": "semistandard --fix",
    "types": "npx -p typescript tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "types": "./types.d.ts",
  "keywords": [],
  "author": "Add00",
  "license": "MPL-2.0",
  "devDependencies": {
    "jasmine-browser-runner": "^2.3.0",
    "jasmine-core": "^5.1.1",
    "jsdoc": "^4.0.3",
    "nyc": "^15.1.0",
    "semistandard": "^17.0.0",
    "typescript": "^5.6.3"
  }
}
